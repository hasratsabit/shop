
	<div class="row">
		{{# each products }}
		<div class="col-sm-6 col-md-4">
			<div class="thumbnail">
				<img src="{{this.imagePath}}" alt="..." class="img-responsive">
				<div class="caption">
					<h3>{{this.title}}</h3>
					<p class="description">{{this.description}}</p>
					<p><span class="price publl-left">${{this.price}}</span> <a href="#" class="btn btn-success pull-right" role="button">Buy Now</a></p>
				</div>
			</div>
		</div>
		{{/each}}
	</div>
